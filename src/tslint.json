{
    "extends": ["../tslint.json", "rxjs-tslint-rules"],
    "rules": {
        "directive-selector": [
            true,
            ["element", "attribute"],
            ["app", "auth", "home", "admin", "questionnaire", "input"],
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            ["app", "auth", "home", "admin", "questionnaire", "forgot", "reset", "iom", "pra", "dashboard", "report"],
            "kebab-case"
        ],
        "rxjs-no-unsafe-takeuntil": {
            "severity": "error"
        }
    }
}
